# Conformance Test: If/Else/Elif Statements
# Tests: if, elif, else, nested if
# Expected: Compiles without errors, correct branch execution

def main():
    x: byte = 5  # Test value for if/else conditions
    if x > 0:
        println("POSITIVE")

    # If-else
    if x > 10:
        println("BIG")
    else:
        println("SMALL")

    # Elif chains (fixed in v0.8.0)
    if x > 7:
        println("LARGE")
    elif x > 3:
        println("MEDIUM")
    else:
        println("TINY")

    # Multiple elif branches
    score: byte = 75
    if score >= 90:
        println("GRADE A")
    elif score >= 80:
        println("GRADE B")
    elif score >= 70:
        println("GRADE C")
    elif score >= 60:
        println("GRADE D")
    else:
        println("GRADE F")

    # Boolean conditions
    flag: bool = true
    if flag:
        println("FLAG SET")

    if not flag:
        println("NOT REACHED")
    else:
        println("FLAG TRUE")

    # Complex conditions
    a: byte = 10
    b: byte = 20
    if a < b and b > 15:
        println("BOTH TRUE")

    if a > 100 or b > 10:
        println("ONE TRUE")
